#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|DETRUDE|PROSPERO|CT|DMG|NODAMAGE" | grep -i -v -Ev "cannonfire|skarvench|matey|borograve|poopdeck|jollyboat|wineflask|carab|leshand|porbuck|blutz|orlop|sailmaster|calidor|toliver|glorianne|corpselight|cookpot|sorcerous|ejada|gunsmoke|mandrigard|gloriannic|splot|capstick|marque|gallowglass|backstreet|curshaw|selenice|jibbers|mutterings|recognize|firepeak|pandanus|grimoire|mortice|scriptor|hmm|leatherbound|skittles|greymalkin|poxy|nobbler|sidonian|shalott|octavius|whipstaff" | grep -i -Ev "d'ye|cap'n|try'n|: ave|: aving|: nothin|: takin|: askin|: tis|: aimin|: goin|: bo'sun|: comin|: wonderin|: ye'll|: ol|: talkin"| uniq
