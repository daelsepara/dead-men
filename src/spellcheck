#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|DETRUDE|PROSPERO|SHALOTT|OCTAVIUS|CT|DMG" | grep -i -v -Ev "cannonfire|skarvench|matey|borograve|poopdeck|jollyboat|wineflask|carab|leshand|porbuck|blutz|orlop|sailmaster|calidor|toliver|glorianne|corpselight|cookpot|sorcerous|ejada|cap'n|gunsmoke|mandrigard|gloriannic|splot|capstick|marque|gallowglass|backstreet|curshaw|try'n|selenice|jibbers|mutterings|recognize|firepeak|pandanus|grimoire|mortice" | grep -i -Ev "d'ye|: ave|: aving|: nothin|: takin|: askin|: tis|: aimin|: goin|: bo'sun|: comin|: wonderin"| uniq
